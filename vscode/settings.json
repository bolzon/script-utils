{
    "editor.insertSpaces": false,
    "editor.autoClosingBrackets": false,
    "editor.fontFamily": "'Liga Fantasque Sans Mono NoLoopK', 'Ligalex Mon', 'Ubuntu Mon Ligaturized', Monaco, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "all",
